{
	"name": "daydream-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"check-node-version": "node .npm-version-check.js",
		"predev": "npm run check-node-version",
		"prebuild": "npm run check-node-version",
		"pretest": "npm run check-node-version",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"add-ts-nocheck": "find src -type f -name \"*.ts\" -o -name \"*.svelte\" -exec grep -l \"<script lang=\\\"ts\\\"\" {} \\; | xargs -I {} sed -i '' '/<script lang=\"ts\">/a\\\n  \\/\\/ @ts-nocheck' {}"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-vercel": "^5.7.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.7",
		"@types/fabric": "^5.3.10",
		"jsdom": "^26.1.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.5",
		"vitest": "^3.1.2"
	},
	"dependencies": {
		"@resvg/resvg-js": "^2.6.2",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.49.4",
		"@sveltejs/kit": "^2.20.7",
		"@tensorflow-models/blazeface": "^0.1.0",
		"@tensorflow-models/coco-ssd": "^2.2.3",
		"@tensorflow/tfjs": "^4.22.0",
		"@tensorflow/tfjs-converter": "^4.22.0",
		"@vercel/analytics": "^1.5.0",
		"@xyflow/svelte": "^0.1.37",
		"axios": "^1.9.0",
		"fabric": "^5.5.2",
		"form-data": "^4.0.2",
		"gsap": "^3.13.0",
		"node-fetch": "^2.7.0",
		"openai": "^4.96.0",
		"perfect-freehand": "^1.2.2",
		"replicate": "^1.0.1",
		"sass": "^1.86.3",
		"undici": "^6.21.2"
	}
}
