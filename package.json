{
	"name": "daydream-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"check-node-version": "node .npm-version-check.js",
		"predev": "npm run check-node-version",
		"prebuild": "npm run check-node-version",
		"pretest": "npm run check-node-version",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"add-ts-nocheck": "find src -type f -name \"*.ts\" -o -name \"*.svelte\" -exec grep -l \"<script lang=\\\"ts\\\"\" {} \\; | xargs -I {} sed -i '' '/<script lang=\"ts\">/a\\\n  \\/\\/ @ts-nocheck' {}"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-vercel": "^5.7.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.7",
		"@types/fabric": "^5.3.10",
		"jsdom": "^26.1.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.5",
		"vitest": "^3.1.2"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.51.0",
		"@browserbasehq/sdk": "^2.6.0",
		"@browserbasehq/stagehand": "^2.2.1",
		"@google/genai": "^1.0.0",
		"@google/generative-ai": "^0.24.1",
		"@langchain/community": "^0.3.44",
		"@langchain/core": "^0.3.57",
		"@langchain/langgraph": "^0.2.73",
		"@langchain/openai": "^0.5.11",
		"@magenta/image": "^0.2.1",
		"@playwright/test": "^1.52.0",
		"@resvg/resvg-js": "^2.6.2",
		"@runware/sdk-js": "^1.1.38",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.49.4",
		"@sveltejs/kit": "^2.20.7",
		"@tensorflow-models/blazeface": "^0.1.0",
		"@tensorflow-models/coco-ssd": "^2.2.3",
		"@tensorflow/tfjs": "^4.22.0",
		"@tensorflow/tfjs-converter": "^4.22.0",
		"@vercel/analytics": "^1.5.0",
		"@xyflow/svelte": "^0.1.37",
		"axios": "^1.9.0",
		"browserbase": "^3.0.3",
		"dotenv": "^16.5.0",
		"fabric": "^5.5.2",
